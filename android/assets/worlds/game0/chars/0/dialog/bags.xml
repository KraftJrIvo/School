<dialog>
  <speechBlock id="0" textEng="Start&#10;" textRus="Начало&#10;" speakerId="" speakerNameEng="" speakerNameRus="">
    <sprite name=""/>
    <link priority="0" textEng="" textRus="" goto="4" condition="playerTookBook"/>
    <link priority="1" textEng="" textRus="" goto="1" condition=""/>
  </speechBlock>
  <speechBlock id="1" textEng="(A pile of bags. Man, are they huge!)
&#10;(What's in them, I wonder)&#10;" textRus="(Сумки навалены. Здоровые какие!)
&#10;(Что в них, интересно)&#10;" speakerId="" speakerNameEng="You" speakerNameRus="Ты">
    <sprite name="bags"/>
    <link priority="0" textEng="" textRus="" goto="2" condition=""/>
  </speechBlock>
  <speechBlock id="2" textEng="(Let's see...)
&#10;(...)
&#10;(Hmm... A lot of books)&#10;" textRus="(Посмотрим...)
&#10;(...)
&#10;(Хмм... Куча книг)&#10;" speakerId="" speakerNameEng="You" speakerNameRus="Ты">
    <sprite name="bags_open"/>
    <link priority="0" textEng="" textRus="" goto="3" condition=""/>
  </speechBlock>
  <speechBlock id="3" textEng="(I'll take one if nobody minds)
&#10;{You said it in your thoughts. Nobody could reply}
&#10;(Pf-f-f you're boring)
&#10;{At least I'm not a thief}&#10;" textRus="(Возьму одну, если никто не против)
&#10;{Ты это сказал про себя. Никто и не мог возразить}
&#10;(Пф-ф-ф зануда)
&#10;{Я хотя бы не вор}&#10;" speakerId="" speakerNameEng="You" speakerNameRus="Ты">
    <sprite name="book"/>
    <link priority="0" textEng="" textRus="" goto="5" condition=""/>
  </speechBlock>
  <speechBlock id="4" textEng="(Bags. They're still full of books)
&#10;(I won't take anymore)
&#10;{Even you have some limits! Who knew...}&#10;" textRus="(Сумки. Они по-прежнему набиты книгами)
&#10;(Больше не буду брать)
&#10;{Даже у тебя есть пределы! Кто бы знал...}&#10;" speakerId="" speakerNameEng="You" speakerNameRus="Ты">
    <sprite name="bags"/>
  </speechBlock>
  <speechBlock id="5" textEng="" textRus="" speakerId="" speakerNameEng="You" speakerNameRus="Ты">
    <sprite name="book"/>
    <varChange charId="0" Id="playerTookBook" value="1"/>
    <itemTransfer to="0" itemName="book" count="1" canGiveEng="[You took a book from one of the bags]" canGiveRus="[Ты взял книгу из одной из сумок]" cantGiveEng="" cantGiveRus=""/>
  </speechBlock>
</dialog>
